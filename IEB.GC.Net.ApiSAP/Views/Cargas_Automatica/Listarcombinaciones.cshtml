                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
               
                    if (item.id_subestacion == 0)
                    {

                    }
                    else
                    {
                        <td>                           
                            @Html.ActionLink("Agregar tipos", "agregarcaso", new { id = item.Id })|
