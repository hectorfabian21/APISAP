@model IEnumerable<IEB.GC.Net.ApiSAP.Models.Casos_de_Combinacione>

@{
    ViewBag.Title = "editarcombinacion";
}

<h2>Editar Tipos</h2>

<table class="table">
    <tr>        
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Caso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Factor_de_modificacion)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Caso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Factor_de_modificacion)
        </td>
        
        <td>
            @Html.ActionLink("Editar", "editarcaso", new { id=item.Id }) |
            @Html.ActionLink("Borrar", "borrarcaso", new { id=item.Id })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Regresar", "Index", new { id_subestacion = int.Parse(TempData.Peek("id").ToString()) })
</div>